{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "eol-last": ["error", "never"],
    "no-shadow": "off",
    "@typescript-eslint/no-shadow": ["off"]
  }
  
}